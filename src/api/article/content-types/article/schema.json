{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Articles",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      }
    },
    "metaTitle": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "summary": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      }
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      }
    },
    "content": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      },
      "inversedBy": "articles"
    },
    "article_type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::article-type.article-type",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      },
      "inversedBy": "articles"
    },
    "article_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article-category.article-category",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      },
      "mappedBy": "articles"
    },
    "article_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article-tag.article-tag",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      },
      "mappedBy": "articles"
    },
    "car_brands": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::car-brand.car-brand",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      },
      "mappedBy": "articles"
    },
    "car_models": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::car-model.car-model",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      },
      "mappedBy": "articles"
    },
    "viewCount": {
      "type": "integer",
      "default": 0
    },
    "engagedSessions": {
      "type": "integer",
      "default": 0
    },
    "featured": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "default": false
    },
    "web_story": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::web-story.web-story",
      "mappedBy": "article",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      }
    },
    "select_related_videos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::car-video.car-video",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      },
      "mappedBy": "select_related_articles"
    },
    "article_comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article-comment.article-comment",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      },
      "mappedBy": "article"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      },
      "component": "shared.seo"
    },
    "helpfulYes": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer"
    },
    "helpfulNo": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer"
    },
    "article_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article-type.article-type",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      },
      "inversedBy": "selected_articles"
    }
  }
}
