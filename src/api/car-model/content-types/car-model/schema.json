{
  "kind": "collectionType",
  "collectionName": "car_models",
  "info": {
    "singularName": "car-model",
    "pluralName": "car-models",
    "displayName": "Car Models",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      }
    },
    "metaTitle": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "car_brands": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::car-brand.car-brand",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      },
      "inversedBy": "car_models"
    },
    "isFeatured": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "isElectricFeatured": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      }
    },
    "car_trims": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::car-trim.car-trim",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      },
      "mappedBy": "car_models"
    },
    "car_sections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::car-section.car-section",
      "mappedBy": "car_models",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      }
    },
    "compare_car": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::compare-car.compare-car",
      "inversedBy": "car_models",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      }
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      },
      "inversedBy": "car_models"
    },
    "highlights": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      },
      "component": "model.model-highlights"
    },
    "key_features": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      },
      "component": "common.key-features"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "translate": {
          "translate": "translate"
        }
      },
      "component": "shared.seo"
    },
    "select_related_videos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::car-video.car-video",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      },
      "mappedBy": "select_related_models"
    },
    "car_reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::car-review.car-review",
      "mappedBy": "car_model",
      "pluginOptions": {
        "translate": {
          "translate": "translate"
        }
      }
    }
  }
}
